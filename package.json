{
  "name": "geely-admin",
  "version": "0.2.54",
  "private": true,
  "scripts": {
    "dev": "npm run start",
    "start": "npm run move:hook & npm run serve:watch",
    "move:hook": "node .hooks/move-hook.js",
    "version-control": "node ./.hooks/version-control.js && git add version.json",
    "serve:watch": "concurrently \"npm run watch\" \"npm run serve\"",
    "serve": "vue-cli-service serve --mode development",
    "watch": "node ./watch.js",
    "build": "npm run generate:route && vue-cli-service build",
    "build:dev": "npm run generate:route && vue-cli-service build --mode develop",
    "build:test": "npm run generate:route && vue-cli-service build --mode test",
    "build:prod": "npm run generate:route && vue-cli-service build --mode prod",
    "build:uat": "npm run generate:route && vue-cli-service build --mode uat",
    "generate:route": "node route-generator.js",
    "lint": "vue-cli-service lint --ext .js,.ts,.vue src",
    "lint-fix": "vue-cli-service lint --fix --ext .js,.ts,.vue src",
    "test:unit": "vue-cli-service test:unit",
    "prettier": "prettier --loglevel silent --write src/**/*.vue src/**/*.js src/**/*.ts src/**/*.scss"
  },
  "dependencies": {
    "@d2-projects/d2-crud": "^2.1.1",
    "@femessage/img-preview": "^1.2.4",
    "@tweenjs/tween.js": "^17.4.0",
    "@types/lodash": "^4.14.135",
    "ali-oss": "^6.1.1",
    "animate.css": "^3.7.2",
    "axios": "^0.19.0",
    "babel-polyfill": "^6.26.0",
    "bignumber.js": "^9.0.0",
    "clipboard": "^2.0.4",
    "co": "^4.6.0",
    "core-js": "^2.6.5",
    "crypto-js": "^3.1.9-1",
    "dayjs": "^1.8.14",
    "echarts": "^4.2.1",
    "element-ui": "~2.10.1",
    "html2canvas": "^1.0.0-rc.3",
    "image-compressor.js": "^1.1.4",
    "lodash": "^4.17.15",
    "nprogress": "^0.2.0",
    "qrcode": "^1.3.3",
    "qrcodejs2": "0.0.2",
    "qs": "^6.7.0",
    "quill": "^1.3.6",
    "resize-observer": "^1.0.0",
    "three": "^0.106.2",
    "v-viewer": "^1.4.2",
    "vue": "^2.6.10",
    "vue-amap": "^0.5.10",
    "vue-class-component": "^7.0.2",
    "vue-clipboard2": "^0.3.1",
    "vue-cropper": "^0.4.9",
    "vue-property-decorator": "^8.1.0",
    "vue-quill-editor": "^3.0.6",
    "vue-router": "3.0.7",
    "vuedraggable": "^2.23.0",
    "vuex": "^3.0.0",
    "vuex-class": "^0.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.4.5",
    "@dcloudio/types": "^0.2.3",
    "@types/chai": "^4.1.0",
    "@types/mocha": "^5.2.4",
    "@vue/cli-plugin-babel": "^3.8.0",
    "@vue/cli-plugin-eslint": "^3.8.0",
    "@vue/cli-plugin-typescript": "^3.8.0",
    "@vue/cli-plugin-unit-mocha": "^3.8.0",
    "@vue/cli-service": "^3.8.0",
    "@vue/eslint-config-prettier": "^4.0.1",
    "@vue/eslint-config-typescript": "^4.0.0",
    "@vue/test-utils": "1.0.0-beta.29",
    "babel-eslint": "^10.0.1",
    "babel-plugin-component": "^1.1.1",
    "babel-plugin-dynamic-import-webpack": "^1.1.0",
    "chai": "^4.1.2",
    "chalk": "^2.4.2",
    "concurrently": "^4.1.1",
    "eslint": "^5.16.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-vue": "^5.0.0",
    "flatted": "^2.0.1",
    "fs-extra": "^8.1.0",
    "lint-staged": "^8.2.1",
    "node-sass": "^4.9.0",
    "path": "^0.12.7",
    "pre-commit": "^1.2.2",
    "sass-loader": "^7.1.0",
    "shelljs": "^0.8.3",
    "svg-sprite-loader": "^4.1.6",
    "text-loader": "^0.0.1",
    "typescript": "~3.4.3",
    "uglifyjs-webpack-plugin": "^2.1.3",
    "vue-cookies": "^1.5.13",
    "vue-i18n": "^8.11.2",
    "vue-template-compiler": "^2.6.10",
    "watch": "^1.0.2",
    "webpack": "^4.34.0"
  },
  "lint-staged": {
    "*.js": [
      "vue-cli-service lint",
      "git add"
    ],
    "*.vue": [
      "vue-cli-service lint",
      "git add"
    ],
    "*.ts": [
      "vue-cli-service lint",
      "git add"
    ]
  },
  "pre-commit": {
    "run": [
      "version-control",
      "lint"
    ],
    "silent": true
  }
}
